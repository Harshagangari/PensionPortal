<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<system.webServer>
		<security>
			<requestFiltering>
				<fileExtensions allowUnlisted="true">
					<remove fileExtension="." />
					<add fileExtension="." allowed="true" />
				</fileExtensions>
			</requestFiltering>
		</security>
		<directoryBrowse enabled="true" showFlags="Date, Time, Size, Extension, LongDate"  />
	</system.webServer>
</configuration>